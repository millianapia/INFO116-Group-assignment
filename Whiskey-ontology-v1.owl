@prefix : <http://www.whisky.com/ontologies/whisky.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.whisky.com/ontologies/whisky.owl> .

<http://www.whisky.com/ontologies/whisky.owl> rdf:type owl:Ontology ;
                                               rdfs:comment "An ontology for the concept whisky" .

#################################################################
#    Object Properties
#################################################################

###  http://www.whisky.com/ontologies/whisky.owl#hasTreatment
<http://www.whisky.com/ontologies/whisky.owl#hasTreatment> rdf:type owl:ObjectProperty ;
                                                                                        owl:inverseOf :treatmentAffects ;
                                                                                        rdfs:domain <http://www.whisky.com/ontologies/whisky.owl#Whisky> ;
                                                                                        rdfs:range <http://www.whisky.com/ontologies/whisky.owl#WhiskyTreatment> ;
                                                                                        rdfs:comment "Describes the treatment given to a whisky" .


###  http://www.whisky.com/ontologies/whisky.owl#producedIn
<http://www.whisky.com/ontologies/whisky.owl#producedIn> rdf:type owl:ObjectProperty ;
                                                                                      owl:inverseOf :areaProduces ;
                                                                                      rdf:type owl:FunctionalProperty ;
                                                                                      rdfs:domain <http://www.whisky.com/ontologies/whisky.owl#Whisky> ;
                                                                                      rdfs:range [ rdf:type owl:Class ;
                                                                                                   owl:unionOf ( <http://www.whisky.com/ontologies/whisky.owl#Country>
                                                                                                                 <http://www.whisky.com/ontologies/whisky.owl#Region>
                                                                                                               )
                                                                                                 ] ;
                                                                                      rdfs:comment "Property to describe which country or region the given whisky was produced in" .


###  http://www.whisky.com/ontologies/whisky.owl#areaProduces
:areaProduces rdf:type owl:ObjectProperty ;
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( <http://www.whisky.com/ontologies/whisky.owl#Country>
                                          <http://www.whisky.com/ontologies/whisky.owl#Region>
                                        )
                          ] ;
              rdfs:range <http://www.whisky.com/ontologies/whisky.owl#Whisky> ;
              rdfs:comment "Describes all the whiskies produced in a given area" .


###  http://www.whisky.com/ontologies/whisky.owl#bottlerProduces
:bottlerProduces rdf:type owl:ObjectProperty ;
                 owl:inverseOf :hasBottler ;
                 rdfs:domain <http://www.whisky.com/ontologies/whisky.owl#WhiskyBottler> ;
                 rdfs:range <http://www.whisky.com/ontologies/whisky.owl#Whisky> ;
                 rdfs:comment "Describes all the whiskies that a given bottler bottles" .


###  http://www.whisky.com/ontologies/whisky.owl#distilledBy
:distilledBy rdf:type owl:ObjectProperty ;
             owl:inverseOf :distillsWhisky ;
             rdf:type owl:FunctionalProperty ;
             rdfs:range :Distillery ;
             rdfs:comment "Describes who destilled the given whisky" .


###  http://www.whisky.com/ontologies/whisky.owl#distillsWhisky
:distillsWhisky rdf:type owl:ObjectProperty ;
                rdfs:range <http://www.whisky.com/ontologies/whisky.owl#Whisky> ;
                rdfs:comment "Describes which whiskies the given destillery destills" .


###  http://www.whisky.com/ontologies/whisky.owl#hasBottler
:hasBottler rdf:type owl:ObjectProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain <http://www.whisky.com/ontologies/whisky.owl#Whisky> ;
            rdfs:range <http://www.whisky.com/ontologies/whisky.owl#WhiskyBottler> ;
            rdfs:comment "Describes who bottled the given whisky" .


###  http://www.whisky.com/ontologies/whisky.owl#hasMaturingType
:hasMaturingType rdf:type owl:ObjectProperty ;
                 owl:inverseOf :maturesWhisky ;
                 rdf:type owl:FunctionalProperty ;
                 rdfs:domain <http://www.whisky.com/ontologies/whisky.owl#Whisky> ;
                 rdfs:range <http://www.whisky.com/ontologies/whisky.owl#MaturingType> ;
                 rdfs:comment "Describes the maturingtype used on a given whisky" .


###  http://www.whisky.com/ontologies/whisky.owl#hasRegion
:hasRegion rdf:type owl:ObjectProperty ;
           owl:inverseOf :isRegionOf ;
           rdfs:domain <http://www.whisky.com/ontologies/whisky.owl#Country> ;
           rdfs:range <http://www.whisky.com/ontologies/whisky.owl#Region> ;
           rdfs:comment "Describes the regions that are a part of a given country" .


###  http://www.whisky.com/ontologies/whisky.owl#isRegionOf
:isRegionOf rdf:type owl:ObjectProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain <http://www.whisky.com/ontologies/whisky.owl#Region> ;
            rdfs:range <http://www.whisky.com/ontologies/whisky.owl#Country> ;
            rdfs:comment "Describes which country the given region is a part of" .


###  http://www.whisky.com/ontologies/whisky.owl#maturesWhisky
:maturesWhisky rdf:type owl:ObjectProperty ;
               rdfs:domain <http://www.whisky.com/ontologies/whisky.owl#MaturingType> ;
               rdfs:range <http://www.whisky.com/ontologies/whisky.owl#Whisky> ;
               rdfs:comment "Describes which whiskies have been matured by a given maturingtype" .


###  http://www.whisky.com/ontologies/whisky.owl#treatmentAffects
:treatmentAffects rdf:type owl:ObjectProperty ;
                  rdfs:domain <http://www.whisky.com/ontologies/whisky.owl#WhiskyTreatment> ;
                  rdfs:range <http://www.whisky.com/ontologies/whisky.owl#Whisky> ;
                  rdfs:comment "Describes the whiskies the given treatment affects" .


#################################################################
#    Data properties
#################################################################

###  http://www.whisky.com/ontologies/whisky.owl#hasABV
<http://www.whisky.com/ontologies/whisky.owl#hasABV> rdf:type owl:DatatypeProperty ,
                                                                                           owl:FunctionalProperty ;
                                                                                  rdfs:range xsd:double ;
                                                                                  rdfs:comment "Property describes the alcohol level (given in percentage)" .


###  http://www.whisky.com/ontologies/whisky.owl#hasAge
<http://www.whisky.com/ontologies/whisky.owl#hasAge> rdf:type owl:DatatypeProperty ,
                                                                                           owl:FunctionalProperty ;
                                                                                  rdfs:range xsd:int ;
                                                                                  rdfs:comment "Property describes the age of a whisky-bottle" .


###  http://www.whisky.com/ontologies/whisky.owl#hasBottlesize
<http://www.whisky.com/ontologies/whisky.owl#hasBottlesize> rdf:type owl:DatatypeProperty ,
                                                                                                  owl:FunctionalProperty ;
                                                                                         rdfs:range xsd:double ;
                                                                                         rdfs:comment "Property describes the volume on the container of the given whisky (given in litres)" .


###  http://www.whisky.com/ontologies/whisky.owl#hasBottlingDate
:hasBottlingDate rdf:type owl:DatatypeProperty ,
                          owl:FunctionalProperty ;
                 rdfs:range xsd:dateTime ;
                 rdfs:comment "Property describes the bottling date of a given whisky (format YYYY-MM-DDThh:mm:ss)" .


###  http://www.whisky.com/ontologies/whisky.owl#hasDistillationDate
:hasDistillationDate rdf:type owl:DatatypeProperty ,
                              owl:FunctionalProperty ;
                     rdfs:range xsd:dateTime ;
                     rdfs:comment "Property describes the distillation date of a given whisky (format YYYY-MM-DDThh:mm:ss)" .


###  http://www.whisky.com/ontologies/whisky.owl#hasName
:hasName rdf:type owl:DatatypeProperty ,
                  owl:FunctionalProperty ;
         rdfs:range xsd:string ;
         rdfs:comment "Describes the name of a given individual" .


